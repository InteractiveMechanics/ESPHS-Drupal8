<div class="subpage-hero-section">
            <div class="container-fluid">
                <div class="row">

                    <div class="col-md-12">
                        
                        <div class="subpage-hero-details generic-hero" style="background-image: linear-gradient(to bottom, rgba(0,0,0,0) 49%,rgba(0,0,0,0.01) 50%,rgba(0,0,0,0.35) 100%), url({{file_url(node.field_lead_image.entity.uri.value)}});">
                            <section>
                                <h1 class="subpage-title">{{node.title.value}}</h1>
                            </section>
                        </div>

                    </div>

                </div>
            </div>
        </div>
        
        
        
        <div class="main-content-subpages press-page">

            <div class="container-fluid">

                <div class="padded-box">
                    
                    <div class="main-item">

                        <div class="row">
                            <div class="col-md-6">
                                
                                {%if press_item[0].field_lead_image.entity.filename %}
                                	<a href="<{# path('entity.node.canonical', {'node': press_item[0].nid.value}) #}">
                                		<img class="img-responsive" src="{{file_url(press_item[0].field_lead_image.entity.uri.value)}}">
                                	</a>
                                {% endif %}
                            </div>

                            <div class="col-md-6">
                                <h6>Latest Press Release</h6>
                                <h2><a href="{{ path('entity.node.canonical', {'node': press_item[0].nid.value}) }}">{{press_item[0].title.value}}</a></h2>
                                {{press_item[0].body.summary|raw}}
                            </div>
                        </div>

                    </div>
                    

                    <div class="row">
                        <div class="col-md-12">
                            <hr />
                        </div>
                    </div>

                    <div class="press-items">

                        <div class="row">
                                
                            <div class="col-md-4">
                                <div>
                                    <div class="press-image">
                                        <a href="{{ path('<front>') }}press-room/press-releases">
                                        	<img class="img-responsive" src="{{ path('<front>') }}themes/historic/assets/press.jpg">
                                        </a>
                                    </div>
                                    <h2><a href="{{ path('<front>') }}press-room/press-releases">Press Releases</a></h2>

                                </div>
                            </div>

                            <div class="col-md-4">
                                <div>
                                    <div class="press-image">
                                        <a href="{{ path('<front>') }}press-room/image-library">
                                        	<img class="img-responsive" src="{{ path('<front>') }}themes/historic/assets/image.jpg">
                                        </a>
									</div>
                                    <h2><a href="{{ path('<front>') }}press-room/image-library">Image Library</a></h2>

                                </div>
                            </div>

                            <div class="col-md-4">
                                <div>
                                    <div class="press-image">
                                        <a href="{{ path('<front>') }}press-room/video-library">
                                        	<img class="img-responsive" src="{{ path('<front>') }}themes/historic/assets/video.jpg">
                                        </a>
                                    </div>
                                    <h2><a href="{{ path('<front>') }}press-room/video-library">Video Library</a></h2>

                                </div>
                            </div>
                                
                        </div>
                    </div>
                    
                    
                {% include directory ~ '/inc/press-contact.html.twig' %}
                    
                </div>

            </div>


        </div>
        
        